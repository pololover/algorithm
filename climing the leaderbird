n = int(input())
ranked = list(map(int,input().split())) 

m = int(input())
player = list(map(int,input().split()))

ranked = sorted(set(ranked), reverse=True)  #set을 쓰면 중복값이 없어진다.
index = len(ranked)
res = []
for a in player:
  while(ranked[index-1] <= a and index > 0):
    index -=1
  
  if(index <0):
    res.append(1)
    continue
  
  res.append(index+1)

for i in range(len(res)):
  print(res[i])



