n = int(input())

count = 0
for i in range(n+1):
  for j in range(60):
    for r in range(60):
      if '3' in str(i) + str(j) + str(r):
        count +=1

print(count)


두번쨰 풀이
#if 문자열 str(num) 기법
n = int(input())

count = 0
for k in range(n+1):
  for i in range(60):
    for j in range(60):
      if '3' in str(k) + str(i) + str(j):
        count+=1


print(count)
